<h2>Products page</h2>
<a href="/products/new">Add a new product</a>
<ul>
  <% products.forEach((product) => { %>
  <li>
    <div class="name"><%= product.name %></div>
    <div class="price">$<%= product.price %></div>
    <a href="/products/delete?id=<%= product.id %>">Delete</a>
  </li>
  <% }); %>
</ul>
